{
	"mappings": [
		{
			"src": "./src/sass/",
			"dest": "./dist/assets/stylesheets/"
		}
	],
	"ignores": [
		"*.css",
		"*.txt",
		"test",
		"path/libs"
	],
	"options": {
		"outputStyle": "expanded",
		"lineComments": false,
		"debugInfo": false,
		"sourceMap": false,
		"unixNewlines": false,
		"autoprefix": true,
		"autoprefixConfig": "> 1%, last 2 versions, Firefox ESR, Opera 12.1"
	},
	"customOptions": [],
	"includePaths": [],
	"requireLibs": []
}